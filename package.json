{
  "name": "ic-auto-tek",
  "version": "1.0.0",
  "description": "Website for IC Auto Tek",
  "main": "index.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development next dev",
    "build": "next build",
    "export": "next export",
    "build-export": "cross-env NODE_ENV=production next build && next export",
    "serve": "npm run build && npm run start-custom",
    "start-custom": "cross-env NODE_ENV=production node server.js",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:server": "cross-env BUNDLE_ANALYZE=server next build",
    "analyze:browser": "cross-env BUNDLE_ANALYZE=browser next build",
    "clean-build": "rimraf \".next\"",
    "clean-export": "rimraf \"out\"",
    "clean": "npm run clean-build && npm run clean-export",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Saw-mon-and-Natalie/ic-auto-tek.git"
  },
  "keywords": [
    "Saw-mon-and-Natalie",
    "IC-AUTO-TEK",
    "Next.js",
    "React"
  ],
  "author": "Saw-mon and Natalie",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Saw-mon-and-Natalie/ic-auto-tek/issues"
  },
  "homepage": "https://github.com/Saw-mon-and-Natalie/ic-auto-tek#readme",
  "dependencies": {
    "@next/bundle-analyzer": "^10.0.4",
    "framer": "^1.2.11",
    "framer-motion": "^3.1.1",
    "intersection-observer": "^0.12.0",
    "locomotive-scroll": "^4.0.6",
    "next": "^10.0.3",
    "next-offline": "^5.0.3",
    "next-seo": "^4.17.0",
    "nextjs-sitemap-generator": "^1.1.3",
    "postcss-easing-gradients": "^3.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-intersection-observer": "^8.31.0",
    "react-redux": "^7.2.2",
    "redux": "^4.0.5",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "autoprefixer": "^10.1.0",
    "babel-plugin-import-glob": "^2.0.0",
    "cross-env": "^7.0.3",
    "postcss": "^8.2.1",
    "react-svg-loader": "^3.0.3",
    "tailwindcss": "^2.0.2"
  }
}
